import requests
import os

api_key = os.getenv('CONGRESS_API_KEY')
if not api_key:
    raise ValueError("CONGRESS_API_KEY environment variable is not set")

url = f'https://api.congress.gov/v3/bill?api_key={api_key}'

response = requests.get(url)

# The response's content will be in JSON format
data = response.json()

# Print the response data to the terminal
print(data)
